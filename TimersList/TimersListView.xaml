<UserControl x:Class="TimersList.TimersListView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:TimersList"
             xmlns:st="clr-namespace:AutoGrid;assembly=SpicyTaco.AutoGrid"
             mc:Ignorable="d"
             d:DesignHeight="400" d:DesignWidth="500">
	<ScrollViewer VerticalScrollBarVisibility="Auto">
		<StackPanel>
			<local:TimersListTimerView MaxHeight="100" MinHeight="50" Padding="5" />
			<local:TimersListTimerView MaxHeight="100" MinHeight="50" Padding="5" Background="#7F808488"/>
			<local:TimersListTimerView MaxHeight="100" MinHeight="50" Padding="5" />
		</StackPanel>
	</ScrollViewer>
</UserControl>